<h2>All Heroines</h2>

<h4>Search by Power </h4>
  <%= form_tag(heroines_path, method: :get) do %>
    <%= text_field_tag(:search, params[:search]) %>
    <%= submit_tag ("Search") %>
  <% end %>


  <% @heroines.each do |heroine| %>
    <li>
      <%= link_to heroine.super_name, heroine_path(heroine) %><br>
    </li>
    <br>
  <% end %>
