<h1>Update Heroine:</h1>
<%= form_for @heroine do |f| %>
    <%= f.label :super_name %>:
    <%= f.text_field :super_name %><br />
  
    <%= f.label :name %>:
    <%= f.text_field :name %><br />


    <%= f.collection_select(:power_id, Power.all, :id, :name) %>
  
    <%= f.submit %>
<% end %>
<br>
<% if flash[:errors] %>
    <% flash[:errors].each do |msg| %>
        <%= msg %><br>
    <% end %>
<% end %>